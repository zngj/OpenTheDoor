{
  "level": "debug",
  "prefix": "[log4g] ",
  "flag": "date|time|shortfile",
  "loggers":[
    {
      "disabled": false,
      "output": "stdout"
    },
    {
      "disabled": true,
      "output": "stderr"
    },
    {
      "disabled": true,
      "output": "file",
      "filename": "logs/log4g.log",
      "maxsize": 100,
      "max_lines": 20000,
      "max_count": 1000,
      "daily": true
    },
    {
      "disabled": true,
      "output": "redis",
      "address": "192.168.56.201:6379",
      "password": "",
      "redis_type": "list",
      "redis_key": "log-list-log4g",
      "codec": "plain",
      "json_key": "message",
      "json_ext": "{\"fields\":{\"env\":\"log4g\"}}"
    },
    {
      "disabled": false,
      "output": "socket",
      "address": "192.168.56.210:5045",
      "network": "udp",
      "codec": "json",
      "json_key": "message",
      "json_ext": "{\"fields\":{\"env\":\"log4g\"}}"
    },
    {
      "disabled": true,
      "output": "elasticsearch"
    }
  ]
}